import java.util.Scanner;

public class Escala {

	public static void main(String[] args)
	{
		Scanner leia = new Scanner(System.in);
		
		double c=0,f=0;
		
		System.out.println("Escreva a temperatura em graus celcius: " + f);
		c =  leia.nextDouble();
		
		f = (c * (9/5))+32;
		
		System.out.print(f);
		
	}	
	
}
