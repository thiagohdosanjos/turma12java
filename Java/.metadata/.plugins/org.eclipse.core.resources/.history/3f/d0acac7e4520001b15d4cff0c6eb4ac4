import java.util.Scanner;

public class Escala {

	public static void main(String[] args)
	{
		Scanner leia = new Scanner(System.in);
		
		double c,f;
		
		System.out.println("Escreva a temperatura em graus celcius: ");
		c =  leia.nextDouble();
		
		f = (c * (9/5))+32;
		
		System.out.print(f);
		
	}	
	
}
